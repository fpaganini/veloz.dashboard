@helper Create(Veloz.Models.IWidgetContent horizontalprogressbarpanel)
{
var panel = (Veloz.Models.IWidgetChart)horizontalprogressbarpanel;
    <div class="x_content">
        <h4>@panel.SubTitle</h4>


        @foreach (var item in panel.Items)
        {
            <div class="widget_summary">
                <div class="w_left w_25">
                    <span>@item.Name</span>
                </div>
                <div class="w_center w_55">
                    <div class="progress">
                        <div class="progress-bar bg-@item.DefaultColor" role="progressbar" aria-valuenow="@item.PercentValue" aria-valuemin="0" aria-valuemax="100" style="width: @item.PercentValue%">
                            <span class="sr-only">@item.PercentValue% Completo</span>
                        </div>
                    </div>
                </div>
                <div class="w_right w_20">
                    <span>@item.Value</span>
                </div>
                <div class="clearfix"></div>
            </div>

        }
    </div>
}
